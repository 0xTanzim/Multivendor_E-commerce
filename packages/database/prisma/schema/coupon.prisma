enum CouponStatus {
  ACTIVE
  INACTIVE
}

model Coupon {
  id         String       @id @default(auto()) @map("_id") @db.ObjectId
  title      String
  couponCode String
  expiryDate DateTime
  isActive   Boolean      @default(true)
  status     CouponStatus
  createdAt  DateTime     @default(now())
  updatedAt  DateTime     @updatedAt
}
