model Training {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  expertId    String
  slug        String
  description String
  image       String
  content     String
  isActive    Boolean

  category   Category @relation(fields: [categoryId], references: [id])
  categoryId String   @db.ObjectId

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
