model User {
  id            String         @id @default(auto()) @map("_id") @db.ObjectId
  name          String?
  gender        String         @default("UNKNOWN")
  bio           String?        @default("")
  dateOfBirth   DateTime?
  address       String?
  settings      Json?
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  authUser      AuthUser?
  products      Product[]
  farmerProfile FarmerProfile?
}
